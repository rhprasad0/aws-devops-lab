# Agent2Agent Guestbook Configuration
# This file is automatically loaded by Terraform (*.auto.tfvars pattern)
# 
# SECURITY: Copy this to guestbook.auto.tfvars and fill in real values
# DO NOT commit guestbook.auto.tfvars to version control

# Enable guestbook infrastructure
enable_guestbook = true

# Resource Names
guestbook_dynamodb_table_name = "a2a-guestbook-messages"
guestbook_secret_name         = "a2a-guestbook/api-keys"

# Kubernetes Configuration
guestbook_namespace       = "guestbook"
guestbook_service_account = "guestbook-sa"

# Initial API Keys
# SECURITY: Generate secure keys with: openssl rand -hex 32
# Store these in AWS Secrets Manager, not in this file
guestbook_initial_api_keys = [
  "REPLACE_WITH_SECURE_KEY_1",
  "REPLACE_WITH_SECURE_KEY_2",
  "REPLACE_WITH_SECURE_KEY_3"
]
